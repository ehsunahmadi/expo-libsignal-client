	"license": "MIT",
	"homepage": "https://github.com/ehsunahmadi/expo-libsignal-client#readme",
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@types/react": "^18.0.25",
		"expo-module-scripts": "^3.4.0",
		"expo-modules-core": "^1.11.2",
		"lefthook": "^1.6.1"
	},
	"peerDependencies": {
		"expo": "*",
		"react": "*",
		"react-native": "*"
	}
