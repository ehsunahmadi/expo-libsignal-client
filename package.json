{
	"name": "expo-libsignal-client",
	"version": "0.1.5",
	"description": "libsignal client as an expo native module",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"scripts": {
		"build": "expo-module build",
		"clean": "expo-module clean",
		"lint": "expo-module lint",
		"test": "expo-module test",
		"prepare": "expo-module prepare",
		"prepublishOnly": "expo-module prepublishOnly",
		"expo-module": "expo-module",
		"open:ios": "open -a \"Xcode\" example/ios",
		"open:android": "open -a \"Android Studio\" example/android",
		"tidy": "npx biome check --apply --no-errors-on-unmatched --files-ignore-unknown=true .",
		"wipe": "npx biome check --apply-unsafe --no-errors-on-unmatched --files-ignore-unknown=true .",
		"postinstall": "lefthook install"
	},
	"keywords": [
		"react-native",
		"expo",
		"expo-libsignal-client",
		"ExpoLibsignalClient"
	],
	"repository": "https://github.com/ehsunahmadi/expo-libsignal-client",
	"bugs": {
		"url": "https://github.com/ehsunahmadi/expo-libsignal-client/issues"
